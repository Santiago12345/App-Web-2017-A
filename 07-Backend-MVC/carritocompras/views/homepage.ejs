
<h1>Homepage</h1>
<div class="row">
  <% for(var i=0; i<usuarios.length; i++) {%>
  <div class="card" style="width: 20rem;">
    <img class="card-img-top" src="http://www.coordinadora.com/wp-content/uploads/sidebar_usuario-corporativo.png" alt="..">
    <div class="card-block">
      <h4 class="card-title"><%=usuarios[i].nombres%></h4>
      <p class="card-text">El apellido de esta persona es el siguiente: <%=usuarios[i].apellidos%></p>
      <form action="/Usuario/eliminarUsuario?id=<%=usuarios[i].id%>" method="post">
        <button  type="submit"
                 class="btn btn-outline-danger">Borrar</button>
      </form>
    </div>
  </div>
  <% } %>
</div>
